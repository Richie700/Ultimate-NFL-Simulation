<?xml version="1.0" encoding="utf-8"?>
<manifest package="lol.footballsimulation"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".AddTeamActivity"/>
        <activity android:name=".ViewTeamActivity"/>
        <activity android:name=".SingleSimulation"/>
        <activity android:name=".SimulationResult"/>
        <activity android:name=".ViewSingleTeamActivity">
        </activity>
    </application>

</manifest>
